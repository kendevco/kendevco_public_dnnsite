  {
    "Title": "Dropdown",
    "Name": "closed-multiple-dropdown",
    "Categories": ["Single Choice"],
    "HelpText": {
      "default": "Allows users to select a a value from a list. On submit, this field will generate the following tokens: [&lt;FieldName&gt;] (which yields the value), [&lt;FieldName&gt;:Text], [<FieldName>:Path], [&lt;FieldName&gt;:Value]. Note that if your options don't have a value (there's nothing after the pipe character in the list below), then all the tokens above will return same value."
    },
    "TypeStr": "avt.ActionForm.Core.Input.MultipleChoice,avt.ActionForm.Core",
    "Flags": ["dropdown"],
    "Parameters": [
      {
        "Id": "LinkTo",
        "Title": {
          "default": "Link to"
        },
        "HelpText": {
          "default": "Link this to another multi-select field that when changed will cause this control to update with different items. For example, imagine Category and Subcategory selection. When this option is used, prefix each item below with the name of the parent category like this: \"Value Of Parent Item/This Item\""
        },
        "DefaultValue": {
          "default": "[]"
        },
        "Type": "MultipleFieldSelect",
        "Settings": {
          "FieldType": [
            "closed-multiple-dropdown",
            "closed-multiple-radio",
            "closed-multiple-checkbox",
            "dropdown-checkboxes",
            "dropdown-autocomplete"
          ]
        }
      },
      {
        "Id": "Empty",
        "Title": {
          "default": "Title When No Selection"
        },
        "HelpText": {
          "default": "It's common to add a \"Please select\" option as the first entry in the dropdown."
        },
        "Type": "Text"
      },
      {
        "Id": "DataSource",
        "Title": {
          "default": "Data Source"
        },
        "HelpText": {
          "default": "Choose from existing data sources or provide values below."
        },
        "DefaultValue": {
          "default": "{ \"Value\": \"Items\", \"Items\": \"First Item\\nSecond Item|WithDifferentValue\" }"
        },
        "Type": "Select",
        "Settings": {
          "DataSource": "FrontEndDataSources"
        }
      },
      {
        "Id": "Value",
        "Title": {
          "default": "Initially Selected"
        },
        "HelpText": {
          "default": "Determines which of the items is initially selected. Possible values are from the list above. Supports Tokens so you can pull data from various sources such as user profile."
        },
        "Type": "Text"
      },
      {
        "Id": "Other",
        "Title": {
          "default": "\"Other\" Option"
        },
        "HelpText": {
          "default": "Optionally, add another option that when selected it will show a textbox where the user can input own data."
        },
        "Type": "Text"
      },
      {
        "Id": "SaveToReport",
        "Title": {
          "default": "Save to Report"
        },
        "HelpText": {
          "default": "The field will be saved in Reports. If Enable Reports from General settings is not checked, this will be overridden."
        },
        "DefaultValue": {
          "default": "false"
        },
        "Type": "Boolean"
      }
    ],
    "Settings": {
      "IsCommon": true
    }
  },
