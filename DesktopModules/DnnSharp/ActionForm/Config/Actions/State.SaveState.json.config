[{
    "Id": "SaveState",
    "Title": {
        "default": "Save State"
    },
    "HelpText": {
        "default": "This action saves all the submitted data in the server session so it can be loaded back into this or another form."
    },
    "TypeStr": "avt.ActionForm.Core.Actions.State.SaveState, avt.ActionForm.Core",
    "Settings": {
        "Group": "Form State"
    },
    "Parameters": [
        {
            "Id": "Key",
            "Title": {
                "default": "Key"
            },
            "HelpText": {
                "default": "Select a key to save under. Note that the scope is global, data can be shared between any two Action Forms. Supports <a href=\"http: //www.dnnsharp.com/dnn/modules/my-custom-tokens\" target=\"_blank\" tabindex=\"-1\">My Tokens</a>"
            },
            "Type": "Text"
        },
        {
            "Id": "SaveLocation",
            "Title": {
                "default": "Save Location"
            },
            "HelpText": {
                "default": "Select where to store the saved data. Session data is lost after a period of inactivity, usually around 20 minutes. Browser cookies usually last longer."
            },
            "Type": "Select",
            "Settings": {
                "Items": {
                    "Session": {
