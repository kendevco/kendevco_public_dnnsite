  </data>
  <data name="lblMessage.Help" xml:space="preserve">
    <value>This is the message which will appear after sending the e-card.</value>
  </data>
  <data name="lblMessage.Text" xml:space="preserve">
    <value>Thank you. Your e-card wil be sent to the recipient.</value>
  </data>
