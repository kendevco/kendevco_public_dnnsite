    <value>Thank you, your e-card will be sent to [Recipient]. Click &lt;a href="[Url]"&gt;here&lt;/a&gt; to return.</value>
  </data>
  <data name="EditModule.Text" xml:space="preserve">
    <value>Manage E-cards</value>
  </data>
  <data name="EmailBody.Text" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html xmlns="http://www.w3.org/1999/xhtml"&gt;
&lt;head&gt;
    &lt;title&gt;An E-Greeting For You&lt;/title&gt;
    &lt;link href="[HOST_NAME]/DesktopModules/Ecards/module.css" rel="stylesheet" /&gt;
&lt;/head&gt;
&lt;body&gt;
    &lt;div style="margin-left: auto; margin-right: auto; width: 814px; padding-top: 30px;"&gt;
        &lt;img alt="" style="width: 424px; height: 45px; border: none;" src="[HOST_NAME]/DesktopModules/Ecards/images/forYou_newWhite.png"&gt;
    &lt;/div&gt;
    
    &lt;table width="814px" cellspacing="0" cellpadding="0" border="0" align="center"&gt;
        &lt;tbody&gt;
            &lt;tr&gt;
                &lt;td colspan="3"&gt;&amp;nbsp;&lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td valign="top"&gt;
                    &lt;img align="left" alt="" style="width: 4px; height: 515px; border: none;" src="[HOST_NAME]/DesktopModules/Ecards/images/shadowLeft.png"&gt;&lt;/td&gt;
                &lt;td valign="top" class="main"&gt;
                    &lt;table cellspacing="0" cellpadding="0" border="0" style="width: 100%;"&gt;
                        &lt;tbody&gt;
                            &lt;tr&gt;
                                &lt;td valign="top" class="left"&gt;
                                    &lt;div class="imageContainer"&gt;
                                        &lt;p style="text-align: center;"&gt;
                                            &lt;img alt="ecard" style="width:100%;max-width:315px;max-height:315px;height:100%;" src="[IMAGE_PATH]"&gt;
                                        &lt;/p&gt;
                                    &lt;/div&gt;
                                    &lt;div style="vertical-align: bottom; text-align: center;"&gt;
                                        &lt;img alt="logo" src="[HOST_NAME]/DesktopModules/Ecards/images/logo_small.jpg"&gt;
                                    &lt;/div&gt;
                                &lt;/td&gt;
                                &lt;td valign="top" class="right"&gt;
                                    &lt;div style="width: 100%; height: 110px; text-align: right; padding-top: 10px;"&gt;
                                        &lt;div style="width: 100%; height: 100%; text-align: right;"&gt;
                                            &lt;div style="margin-right: 40px;"&gt;
                                                &lt;img style="width: 226px; height: 106px;" alt="" src="[HOST_NAME]/DesktopModules/Ecards/images/CH_Stamp2.jpg"&gt;
                                            &lt;/div&gt;
                                        &lt;/div&gt;
                                    &lt;/div&gt;
                                    &lt;div class="address" style="padding-left: 10px"&gt;
                                        &lt;table cellspacing="0" cellpadding="0" border="0"&gt;
                                            &lt;tbody&gt;
                                                &lt;tr&gt;
                                                    &lt;td valign="top" style="text-align: left; padding-left: 10px;"&gt;
                                                        &lt;div class="cardlabel" style="padding: 0 10px 20px 0;"&gt;Recipient Name :&lt;/div&gt;
                                                    &lt;/td&gt;
                                                    &lt;td valign="top"&gt;
                                                        [RECIPIENT_NAME]
                                                    &lt;/td&gt;
                                                &lt;/tr&gt;
                                                &lt;tr&gt;
                                                    &lt;td valign="top" style="text-align: left; padding-left: 10px;"&gt;
                                                        &lt;div class="cardlabel" style="padding: 0 10px 20px 0;"&gt;Sender Name :&lt;/div&gt;
                                                    &lt;/td&gt;
                                                    &lt;td valign="top"&gt;
                                                        [SENDER_NAME]
                                                    &lt;/td&gt;
                                                &lt;/tr&gt;
                                                &lt;tr&gt;
                                                    &lt;td valign="top" style="text-align: left; padding-left: 10px;"&gt;
                                                        &lt;div class="cardlabel" style="padding: 0 10px 20px 0;"&gt;Sender Email :&lt;/div&gt;
                                                    &lt;/td&gt;
                                                    &lt;td valign="top"&gt;
                                                        [EMAIL]
                                                    &lt;/td&gt;
                                                &lt;/tr&gt;
                                                &lt;tr&gt;
                                                    &lt;td valign="top" style="text-align: left; padding-left: 10px;"&gt;
                                                        &lt;div class="cardlabel" style="padding: 0 10px 20px 0;"&gt;Message :&lt;/div&gt;
                                                    &lt;/td&gt;
                                                    &lt;td valign="top"&gt;
                                                        [MESSAGE]
                                                    &lt;/td&gt;
                                                &lt;/tr&gt;
                                            &lt;/tbody&gt;
                                        &lt;/table&gt;
                                    &lt;/div&gt;
                                &lt;/td&gt;
                            &lt;/tr&gt;
                        &lt;/tbody&gt;
                    &lt;/table&gt;
                &lt;/td&gt;
                &lt;td valign="top"&gt;
                    &lt;img align="right" alt="" style="width: 5px; height: 515px; border: none;" src="[HOST_NAME]/DesktopModules/Ecards/images/shadowRight.png"&gt;&lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td valign="top" colspan="3" style="line-height: 0"&gt;
                    &lt;img alt="" style="width: 814px; height: 7px; border: none;" src="[HOST_NAME]/DesktopModules/Ecards/images/shadowBtm.png"&gt;&lt;/td&gt;
            &lt;/tr&gt;
        &lt;/tbody&gt;
    &lt;/table&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
